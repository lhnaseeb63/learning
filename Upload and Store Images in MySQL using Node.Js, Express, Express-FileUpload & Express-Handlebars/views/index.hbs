<form action="/" method="post" enctype="multipart/form-data">
  <h3>Upload Profile Photo</h3>
  <input type="file" name="sampleFile" accept="image/*" />
  <input type="submit" class="btn btn-primary" />

</form>

<div class="card">
  {{#each rows}}

    {{#if this.profileImage}}
      <img class="class__image image" src="{{this.profileImage}}" alt="User Profile"/>
    {{else}}
      <img class="class__image image" src="/img/default.jpg" alt="User Profile"/>
    {{/if}}

    <h1 class="card__title title">{{this.name}}</h1>
    <div class="card__job job">{{this.job_title}}</div>
    <p class="card__about">{{this.description}}</p>
    <button type="button" class="btn btn-primary">View Profile</button>

  {{/each}}
</div>